
    <div class="row">
      <div class="large-12 columns">
        <div class="row">


            <% @babies.each do |baby| %>
            <div class="large-3 small-6 columns end">
              <%= image_tag "mashabear.jpg", size:"150x150" %>
              <h6 class="panel"><p><strong> First name: </strong><%= baby.first_name %> <br>
              <strong>Date of Birth:</strong> <%= baby.dob %></p></h6>
            </div>
            <% end %>

            Paychecks:

            <% @paychecks.each do |paycheck| %>
              <h6><p><strong> For the week: </strong><%= paycheck.start_date %>-<%= paycheck.end_date %> <br>
              <strong>Amount:</strong> $ <%= paycheck.hr_base*paycheck.hr_qty %></p></h6>
            </div>
            <% end %>




          </div>
        </div>
      </div>
